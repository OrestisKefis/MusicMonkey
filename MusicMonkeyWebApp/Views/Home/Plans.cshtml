  }
  .plans__table th:last-child,
  .plans__table td:last-child {
    padding-right: 0;
  }
  .plans__table tbody tr:last-child td {
    padding: 20px 15px 0 25px;
  }
  .plans__table tbody tr:last-child td:last-child {
    padding-right: 0;
