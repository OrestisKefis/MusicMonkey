<script>
    $(document).ready(() => {
        showplans();
    });
    let showplans = () => {
        let subscriptions = [
            { name: 'Free', price: '0' },
            { name: 'Silver', price: '29,99' },
            { name: 'Gold', price: '49,99' },
        ];
        $('#subscriptions').html(subscriptions.map(o =>
            ` <th>
                <div class="plans__head">
                    <b>${o.name} MEMBER</b>
                    <p>${o.price}</p>
                    <span>/ month</span>
                </div>
            </th>`
        ));
    };
</script>
